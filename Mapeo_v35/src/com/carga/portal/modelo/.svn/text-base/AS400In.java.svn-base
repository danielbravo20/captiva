package com.carga.portal.modelo;

public class AS400In {
	private String codigoPrograma;
	private String codigoAtributo;
	private String inicio;
	private String longitud;
	private String tipoDato;
	private String formatoDate;
	private String escala;
	private String descripcion;
	private String valorDefecto;
	private String atributoOrigen;
	private boolean filtro;
	private String longitudLista;
	
	public String getCodigoPrograma() {
		return codigoPrograma;
	}
	public void setCodigoPrograma(String codigoPrograma) {
		this.codigoPrograma = codigoPrograma;
	}
	public String getCodigoAtributo() {
		return codigoAtributo;
	}
	public void setCodigoAtributo(String codigoAtributo) {
		this.codigoAtributo = codigoAtributo;
	}
	public String getInicio() {
		return inicio;
	}
	public void setInicio(String inicio) {
		this.inicio = inicio;
	}
	public String getLongitud() {
		return longitud;
	}
	public void setLongitud(String longitud) {
		this.longitud = longitud;
	}
	public String getTipoDato() {
		return tipoDato;
	}
	public void setTipoDato(String tipoDato) {
		this.tipoDato = tipoDato;
	}
	public String getFormatoDate() {
		return formatoDate;
	}
	public void setFormatoDate(String formatoDate) {
		this.formatoDate = formatoDate;
	}
	public String getEscala() {
		return escala;
	}
	public void setEscala(String escala) {
		this.escala = escala;
	}
	public String getDescripcion() {
		return descripcion;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	public String getValorDefecto() {
		return valorDefecto;
	}
	public void setValorDefecto(String valorDefecto) {
		this.valorDefecto = valorDefecto;
	}
	public String getAtributoOrigen() {
		return atributoOrigen;
	}
	public void setAtributoOrigen(String atributoOrigen) {
		this.atributoOrigen = atributoOrigen;
	}
	public boolean isFiltro() {
		return filtro;
	}
	public void setFiltro(boolean filtro) {
		this.filtro = filtro;
	}
	public void setLongitudLista(String longitudLista) {
		this.longitudLista = longitudLista;
	}
	public String getLongitudLista() {
		return longitudLista;
	}
	
}