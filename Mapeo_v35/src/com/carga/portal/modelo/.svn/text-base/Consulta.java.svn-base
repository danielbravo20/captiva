package com.carga.portal.modelo;

import java.util.List;

public class Consulta {

	private int codigo;
	private String aleas;
	private String paquete;
	private String interfase;
	private TablaConsulta tablaPadre;
	private List<TablaConsulta> tablasFK;
	
	public int getCodigo() {
		return codigo;
	}
	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	public String getAleas() {
		return aleas;
	}
	public void setAleas(String aleas) {
		this.aleas = aleas;
	}
	public String getPaquete() {
		return paquete;
	}
	public void setPaquete(String paquete) {
		this.paquete = paquete;
	}
	public String getInterfase() {
		return interfase;
	}
	public void setInterfase(String interfase) {
		this.interfase = interfase;
	}
	public TablaConsulta getTablaPadre() {
		return tablaPadre;
	}
	public void setTablaPadre(TablaConsulta tablaPadre) {
		this.tablaPadre = tablaPadre;
	}
	public List<TablaConsulta> getTablasFK() {
		return tablasFK;
	}
	public void setTablasFK(List<TablaConsulta> tablasFK) {
		this.tablasFK = tablasFK;
	}
	
}
