package com.carga.portal.modelo;

import java.util.List;

public class ObjetoBPM {
	
	private Integer codigo;
	private String nombre;
	private List<AtributoTarea> atributosTarea;
	private Clase clase;
	
	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public Integer getCodigo() {
		return codigo;
	}

	public void setCodigo(Integer codigo) {
		this.codigo = codigo;
	}

	public List<AtributoTarea> getAtributosTarea() {
		return atributosTarea;
	}

	public void setAtributosTarea(List<AtributoTarea> atributosTarea) {
		this.atributosTarea = atributosTarea;
	}

	public Clase getClase() {
		return clase;
	}

	public void setClase(Clase clase) {
		this.clase = clase;
	}
	
	
}
